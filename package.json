{
  "private": true,
  "name": "@akashic/playground",
  "version": "1.0.0",
  "description": "",
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akashic-games/akashic-playground.git"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run format && run-s build:*",
    "build:webpack": "webpack --config webpack.prod.js",
    "update-engines": "node scripts/update_akashic_versions_json.js",
    "format": "run-s format:*",
    "format:prettier": "prettier --write \"src/**/*.{ts,vue}\" --config prettier.config.json",
    "format:eslint": "eslint \"src/**/*.{ts,vue}\" --fix",
    "generate": "run-s generate:*",
    "generate:types": "node scripts/generate_akashic_definitions.js",
    "test": "run-s test:*",
    "test:eslint": "eslint \"src/**/*.{ts,vue}\"",
    "test:unit": "jest",
    "deploy": "npm run build && gh-pages -d public",
    "dev": "webpack-dev-server --content-base public/ --config webpack.dev.js",
    "storybook": "start-storybook -s ./public,./stories/fixtures,./src/assets -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/vue": "^6.0.26",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^27.0.2",
    "@types/sanitize-html": "^1.27.0",
    "@types/url-join": "^4.0.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@vue/composition-api": "^1.0.0-beta.14",
    "@vue/test-utils": "^1.1.0",
    "axios": "^0.20.0",
    "axios-mock-adapter": "^1.18.2",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "css-loader": "^4.3.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-unused-imports": "^0.1.3",
    "eslint-plugin-vue": "^7.0.1",
    "file-loader": "^6.1.0",
    "file-saver": "^2.0.2",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.5.2",
    "js-base64": "^3.5.2",
    "jszip": "^3.5.0",
    "jszip-utils": "^0.1.0",
    "marked": "^1.2.0",
    "material-design-icons": "^3.0.1",
    "monaco-editor": "^0.21.2",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "sass-loader": "^10.0.4",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "url-join": "^4.0.1",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.4.5",
    "vue-svg-loader": "^0.17.0-beta.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  }
}
